{
  "tools": [
    {
      "name": "Terser",
      "npm": {
        "package": "terser",
        "version": "4.0.0"
      },
      "execCommand": "./terser",
      "execArguments": "%INPUT_FILE%"
    },
    {
      "name": "esmangle",
      "npm": {
        "package": "esmangle",
        "version": "1.0.1"
      },
      "execCommand": "./esmangle",
      "execArguments": "%INPUT_FILE%"
    },
    {
      "name": "babel-minify (beta)",
      "npm": {
        "package": "babel-minify",
        "version": "0.5.0"
      },
      "execCommand": "./minify",
      "execArguments": "%INPUT_FILE%"
    },
    {
      "name": "Google Closure Compiler",
      "download": {
        "url": "https://dl.google.com/closure-compiler/compiler-20190528.zip",
        "fileName": "googleclosurecompiler.zip"
      },
      "execCommand": "java",
      "execArguments": " -jar closure-compiler-v20190528.jar %INPUT_FILE%"
    },
    {
      "name": "Njsast",
      "gitSource": {
        "url": "https://github.com/miroslavpokorny/NjsastRunner.git",
        "buildCommand": "dotnet publish -c Release NjsastRunner/NjsastRunner.csproj -o ./bin"
      },
      "execCommand": "dotnet",
      "execArguments": "./bin/NjsastRunner.dll %INPUT_FILE%"
    }
  ]
}